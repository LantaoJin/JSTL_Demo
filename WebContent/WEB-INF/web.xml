<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Servlet</display-name>
  <welcome-file-list>
    <welcome-file>Dashboard.jsp</welcome-file>
</welcome-file-list>
  <servlet>
	  <servlet-name>EmployeeServlet</servlet-name>
	  <servlet-class>rp.servlets.rajan.EmployeeServlet</servlet-class>
	  <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
	  <servlet-name>RajanServlet</servlet-name>
	  <servlet-class>rp.servlets.rajan.ExampleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>EmployeeServlet</servlet-name>
	  <url-pattern>/RajanServlet</url-pattern>
  </servlet-mapping>
</web-app>